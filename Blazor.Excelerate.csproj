  </ItemGroup>

  <ItemGroup>
    <Folder Include="wwwroot\Downloads\Classes\" />
  </ItemGroup>

  <ItemGroup>
    <None Include="wwwroot\Images\ExcelerateLogoSmallWhite.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>

</Project>
